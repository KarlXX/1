#include<stdio.h>
#include <math.h>
#include <stdlib.h>
#include<iostream>

using namespace std;

double array[5] = {0.11846344, 0.2393143, 0.2844444, 0.2393143, 0.11846344};
double array2[5] = {0.04691008, 0.23076534, 0.5, 0.76923466, 0.95308992};

double func(double x){
	return sin(x);}

double func2(double X, int i){
	int a, n;
	double b;
	a = 0;
	b = 1.570796326;
	return X = a + (b - a)* array2[i];}

int main(){
	int a, n;
	double z, Result, b;
	a = 0;
	b = 1.570796326;
	n = 5;
	z = 0;
	

	for(int i = 0; i <= n; i++){
		
		z += array[i] * func(func2(z, i));
	}
	Result = (b - a)* z;
	printf("%.8f", Result);

	system("pause");
return 0;
}
